# SEO Headings Structure Analyzer - Environment Variables
# Copy this file to .env and modify as needed

# User Agent for HTTP requests
USER_AGENT=Mozilla/5.0 (compatible; SEO-Headings-Analyzer/1.0)

# Default timeout for page requests (in milliseconds)
DEFAULT_TIMEOUT=10000

# Maximum number of redirects to follow
DEFAULT_MAX_REDIRECTS=5

# Whether to include heading text content
INCLUDE_HEADING_TEXT=true

# Whether to include detailed heading structure analysis
INCLUDE_HEADING_STRUCTURE=true

# Whether to calculate and include heading scores
INCLUDE_HEADING_SCORE=true
